      "@storm-stack/hooks": "workspace:*",
      "@storm-stack/env": "workspace:*",
      "unplugin-storm-stack": "workspace:*",
      "@storm-stack/build": "workspace:*"
    }
  },
  "prettier": "@storm-software/prettier/config.json",
  "triggerEmptyDevReleaseByIncrementingThisNumber": 0
