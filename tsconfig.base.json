{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@storm-software/tsconfig/core.json",
  "display": "Storm Software - Storm Stack",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@storm-stack/biome": ["packages/biome/src/index.ts"],
      "@storm-stack/biome/*": ["packages/biome/src/*"],
      "@storm-stack/cli": ["packages/cli/src/index.ts"],
      "@storm-stack/cli/*": ["packages/cli/src/*"],
      "@storm-stack/core": ["packages/core/src/index.ts"],
      "@storm-stack/core/runtime-types/*": ["packages/core/runtime-types/*"],
      "@storm-stack/core/*": ["packages/core/src/*"],
      "@storm-stack/devkit": ["packages/devkit/src/index.ts"],
      "@storm-stack/devkit/*": ["packages/devkit/src/*"],
      "@storm-stack/examples-cli-app": ["examples/cli-app/src/index.ts"],
      "@storm-stack/examples-cli-app/*": ["examples/cli-app/src/*"],
      "@storm-stack/examples-cloudflare-worker": [
        "examples/cloudflare-worker/src/index.ts"
      ],
      "@storm-stack/examples-cloudflare-worker/*": [
        "examples/cloudflare-worker/src/*"
      ],
      "@storm-stack/examples-library": ["examples/library/src/index.ts"],
      "@storm-stack/examples-library/*": ["examples/library/src/*"],
      "@storm-stack/nx": ["packages/nx/index.ts"],
      "@storm-stack/nx/*": ["packages/nx/src/*"],
      "@storm-stack/plugin-cli": ["packages/plugin-cli/src/index.ts"],
      "@storm-stack/plugin-cli/*": ["packages/plugin-cli/src/*"],
      "@storm-stack/plugin-cloudflare": [
        "packages/plugin-cloudflare/src/index.ts"
      ],
      "@storm-stack/plugin-cloudflare/*": ["packages/plugin-cloudflare/src/*"],
      "@storm-stack/plugin-config": ["packages/plugin-config/src/index.ts"],
      "@storm-stack/plugin-config/*": ["packages/plugin-config/src/*"],
      "@storm-stack/plugin-date": ["packages/plugin-date/src/index.ts"],
      "@storm-stack/plugin-date/*": ["packages/plugin-date/src/*"],
      "@storm-stack/plugin-error": ["packages/plugin-error/src/index.ts"],
      "@storm-stack/plugin-error/*": ["packages/plugin-error/src/*"],
      "@storm-stack/plugin-log-console": [
        "packages/plugin-log-console/src/index.ts"
      ],
      "@storm-stack/plugin-log-console/*": [
        "packages/plugin-log-console/src/*"
      ],
      "@storm-stack/plugin-log-otel": ["packages/plugin-log-otel/src/index.ts"],
      "@storm-stack/plugin-log-otel/*": ["packages/plugin-log-otel/src/*"],
      "@storm-stack/plugin-log-sentry": [
        "packages/plugin-log-sentry/src/index.ts"
      ],
      "@storm-stack/plugin-log-sentry/*": ["packages/plugin-log-sentry/src/*"],
      "@storm-stack/plugin-log-storage": [
        "packages/plugin-log-storage/src/index.ts"
      ],
      "@storm-stack/plugin-log-storage/*": [
        "packages/plugin-log-storage/src/*"
      ],
      "@storm-stack/plugin-node": ["packages/plugin-node/src/index.ts"],
      "@storm-stack/plugin-node/*": ["packages/plugin-node/src/*"],
      "@storm-stack/plugin-react": ["packages/plugin-react/src/index.ts"],
      "@storm-stack/plugin-react/*": ["packages/plugin-react/src/*"],
      "@storm-stack/plugin-storage-cloudflare-kv": [
        "packages/plugin-storage-cloudflare-kv/src/index.ts"
      ],
      "@storm-stack/plugin-storage-cloudflare-kv/*": [
        "packages/plugin-storage-cloudflare-kv/src/*"
      ],
      "@storm-stack/plugin-storage-cloudflare-r2": [
        "packages/plugin-storage-cloudflare-r2/src/index.ts"
      ],
      "@storm-stack/plugin-storage-cloudflare-r2/*": [
        "packages/plugin-storage-cloudflare-r2/src/*"
      ],
      "@storm-stack/plugin-storage-fs": [
        "packages/plugin-storage-fs/src/index.ts"
      ],
      "@storm-stack/plugin-storage-fs/*": ["packages/plugin-storage-fs/src/*"],
      "@storm-stack/plugin-storage-s3": [
        "packages/plugin-storage-s3/src/index.ts"
      ],
      "@storm-stack/plugin-storage-s3/*": ["packages/plugin-storage-s3/src/*"],
      "@storm-stack/tools-config/*": ["tools/config/*"],
      "@storm-stack/tools-nx": ["tools/nx/src/index.ts"],
      "@storm-stack/tools-nx/*": ["tools/nx/src/*"],
      "@storm-stack/tools-scripts": ["tools/scripts/src/index.ts"],
      "@storm-stack/tools-scripts/*": ["tools/scripts/src/*"],
      "@storm-stack/tsdoc": ["packages/tsdoc/src/index.ts"],
      "@storm-stack/tsdoc/*": ["packages/tsdoc/src/*"],
      "@storm-stack/types": ["packages/types/src/index.ts"],
      "@storm-stack/types/*": ["packages/types/src/*"],
      "eslint-config-storm-stack": ["packages/eslint-config/src/index.ts"],
      "eslint-config-storm-stack/*": ["packages/eslint-config/src/*"],
      "eslint-plugin-storm-stack": ["packages/eslint-plugin/src/index.ts"],
      "eslint-plugin-storm-stack/*": ["packages/eslint-plugin/src/*"]
    },
    "verbatimModuleSyntax": false
  }
}
