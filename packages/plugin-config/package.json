      "import": {
        "types": "./dist/templates/index.d.ts",
        "default": "./dist/templates/index.js"
      },
      "require": {
        "types": "./dist/templates/index.d.cts",
        "default": "./dist/templates/index.cjs"
      },
      "default": {
        "types": "./dist/templates/index.d.ts",
        "default": "./dist/templates/index.js"
      }
    },
    "./templates/*": {
      "import": {
        "types": "./dist/templates/*.d.ts",
        "default": "./dist/templates/*.js"
      },
      "require": {
        "types": "./dist/templates/*.d.cts",
        "default": "./dist/templates/*.cjs"
      },
      "default": {
        "types": "./dist/templates/*.d.ts",
        "default": "./dist/templates/*.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "storm-stack-plugin"
  ],
  "dependencies": {
    "@babel/core": "catalog:",
