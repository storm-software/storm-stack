    "@stryke/type-checks": "catalog:",
    "@stryke/types": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/nx": "workspace:*",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
