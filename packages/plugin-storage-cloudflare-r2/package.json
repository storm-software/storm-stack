{
  "name": "@storm-stack/plugin-storage-cloudflare-r2",
  "version": "0.3.1",
  "type": "module",
  "description": "A package containing a Storm Stack storage adapter to write to Cloudflare R2",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-storage-cloudflare-r2"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "storm-stack-plugin",
    "storm-stack-storage-plugin"
  ],
  "peerDependencies": {
    "aws4fetch": "^1.0.20"
  },
  "peerDependenciesMeta": {
    "aws4fetch": {
      "optional": true
    }
  },
  "dependencies": {
    "@ltd/j-toml": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-stack/core": "workspace:*",
    "@storm-stack/devkit": "workspace:*",
    "@stryke/fs": "catalog:",
    "@stryke/path": "catalog:",
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/nx": "workspace:*",
    "@types/node": "catalog:",
    "tsup": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
