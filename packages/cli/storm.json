{
  "name": "Storm Stack",
  "skipCache": true,
  "plugins": [
    [
      "@storm-stack/plugin-cli",
      {
        "bin": ["storm", "storm-stack"],
        "dotenv": {
          "types": {
            "config": "./src/types.ts#StormStackCLIConfig"
          }
        }
      }
    ],
    [
      "@storm-stack/plugin-log-storage",
      {
        "logLevel": "info"
      }
    ],
    [
      "@storm-stack/plugin-log-sentry",
      {
        "logLevel": "error"
      }
    ]
  ]
}
