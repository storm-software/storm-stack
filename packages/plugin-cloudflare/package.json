{
  "name": "@storm-stack/plugin-cloudflare",
  "version": "0.27.1",
  "type": "module",
  "description": "A Storm Stack plugin for managing deployments to Cloudflare",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-cloudflare"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "require": {
        "types": "./dist/plugin.d.cts",
        "default": "./dist/plugin.cjs"
      },
      "default": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      }
    },
    "./types/*": {
      "import": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.js"
      },
      "require": {
        "types": "./dist/types/*.d.cts",
        "default": "./dist/types/*.cjs"
      },
      "default": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.js"
      }
    }
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@cloudflare/unenv-preset": "catalog:",
    "@cloudflare/vite-plugin": "catalog:",
    "@ltd/j-toml": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-stack/core": "0.41.1",
    "@storm-stack/types": "workspace:*",
    "@stryke/fs": "catalog:",
    "@stryke/json": "catalog:",
    "@stryke/path": "catalog:",
    "defu": "catalog:",
    "unenv": "catalog:"
  },
  "devDependencies": {
    "@stryke/types": "catalog:",
    "tsup": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
