{
  "name": "@storm-stack/plugin-cloudflare-worker",
  "version": "0.21.1",
  "type": "module",
  "description": "A Storm Stack build plugin for Cloudflare Workers",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-cloudflare-worker"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "require": {
        "types": "./dist/plugin.d.cts",
        "default": "./dist/plugin.cjs"
      },
      "default": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      }
    }
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "@storm-stack/plugin-node": "*"
  },
  "peerDependenciesMeta": {
    "@storm-stack/plugin-node": {
      "optional": false
    }
  },
  "dependencies": {
    "@cloudflare/unenv-preset": "catalog:",
    "@ltd/j-toml": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-stack/core": "workspace:*",
    "@stryke/fs": "catalog:",
    "@stryke/json": "catalog:",
    "@stryke/path": "catalog:",
    "defu": "catalog:",
    "unenv": "catalog:"
  },
  "devDependencies": {
    "@stryke/types": "catalog:",
    "tsup": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
