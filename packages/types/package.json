{
  "name": "@storm-stack/types",
  "version": "0.18.0",
  "description": "A package containing shared TypeScript type definitions used in Storm Stack projects.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/types"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      },
      "default": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/esm/src/shared/*.d.ts",
        "default": "./dist/esm/src/shared/*.js"
      },
      "require": {
        "types": "./dist/cjs/src/shared/*.d.ts",
        "default": "./dist/cjs/src/shared/*.js"
      },
      "default": {
        "types": "./dist/esm/src/shared/*.d.ts",
        "default": "./dist/esm/src/shared/*.js"
      }
    },
    "./shared": {
      "import": {
        "types": "./dist/esm/src/shared/index.d.ts",
        "default": "./dist/esm/src/shared/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/shared/index.d.ts",
        "default": "./dist/cjs/src/shared/index.js"
      },
      "default": {
        "types": "./dist/esm/src/shared/index.d.ts",
        "default": "./dist/esm/src/shared/index.js"
      }
    },
    "./shared/*": {
      "import": {
        "types": "./dist/esm/src/shared/*.d.ts",
        "default": "./dist/esm/src/shared/*.js"
      },
      "require": {
        "types": "./dist/cjs/src/shared/*.d.ts",
        "default": "./dist/cjs/src/shared/*.js"
      },
      "default": {
        "types": "./dist/esm/src/shared/*.d.ts",
        "default": "./dist/esm/src/shared/*.js"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/esm/src/node/index.d.ts",
        "default": "./dist/esm/src/node/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/node/index.d.ts",
        "default": "./dist/cjs/src/node/index.js"
      },
      "default": {
        "types": "./dist/esm/src/node/index.d.ts",
        "default": "./dist/esm/src/node/index.js"
      }
    },
    "./node/*": {
      "import": {
        "types": "./dist/esm/src/node/*.d.ts",
        "default": "./dist/esm/src/node/*.js"
      },
      "require": {
        "types": "./dist/cjs/src/node/*.d.ts",
        "default": "./dist/cjs/src/node/*.js"
      },
      "default": {
        "types": "./dist/esm/src/node/*.d.ts",
        "default": "./dist/esm/src/node/*.js"
      }
    },
    "./browser": {
      "import": {
        "types": "./dist/esm/src/browser/index.d.ts",
        "default": "./dist/esm/src/browser/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/browser/index.d.ts",
        "default": "./dist/cjs/src/browser/index.js"
      },
      "default": {
        "types": "./dist/esm/src/browser/index.d.ts",
        "default": "./dist/esm/src/browser/index.js"
      }
    },
    "./browser/*": {
      "import": {
        "types": "./dist/esm/src/browser/*.d.ts",
        "default": "./dist/esm/src/browser/*.js"
      },
      "require": {
        "types": "./dist/cjs/src/browser/*.d.ts",
        "default": "./dist/cjs/src/browser/*.js"
      },
      "default": {
        "types": "./dist/esm/src/browser/*.d.ts",
        "default": "./dist/esm/src/browser/*.js"
      }
    }
  },
  "types": "./dist/cjs/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "types",
    "typescript"
  ],
  "dependencies": {
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "@storm-software/build-tools": "catalog:",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
