{
  "name": "file-system",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/file-system/src",
  "targets": {
    "build": {
      "executor": "@storm-software/workspace-tools:tsup-node",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/file-system",
        "tsConfig": "packages/file-system/tsconfig.json",
        "project": "packages/file-system/package.json",
        "defaultConfiguration": "production",
        "platform": "node"
      },
      "configurations": {
        "production": {
          "debug": false,
          "verbose": true
        },
        "development": {
          "debug": true,
          "verbose": true
        }
      }
    },
    "lint": {},
    "test": {},
    "nx-release-publish": {
      "cache": false,
      "inputs": ["default", "^production"],
      "dependsOn": ["^build"],
      "executor": "@storm-software/workspace-tools:npm-publish",
      "options": {
        "packageRoot": "dist/{projectRoot}",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "implicitDependencies": ["cli", "utilities", "logging", "errors", "serialization"]
}
