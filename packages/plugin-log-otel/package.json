{
  "name": "@storm-stack/plugin-log-otel",
  "version": "0.15.0",
  "type": "module",
  "description": "A package containing a Storm Stack log adapter to write logs to an OpenTelemetry collector",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-log-otel"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      },
      "default": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "storm-stack-plugin",
    "storm-stack-log-plugin"
  ],
  "peerDependencies": {
    "@opentelemetry/api-logs": "catalog:",
    "@opentelemetry/exporter-logs-otlp-http": "catalog:",
    "@opentelemetry/otlp-exporter-base": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-logs": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api-logs": {
      "optional": true
    },
    "@opentelemetry/exporter-logs-otlp-http": {
      "optional": true
    },
    "@opentelemetry/otlp-exporter-base": {
      "optional": true
    },
    "@opentelemetry/resources": {
      "optional": true
    },
    "@opentelemetry/sdk-logs": {
      "optional": true
    },
    "@opentelemetry/semantic-conventions": {
      "optional": true
    }
  },
  "dependencies": {
    "@storm-stack/core": "workspace:*",
    "@storm-stack/devkit": "workspace:*",
    "@stryke/json": "catalog:"
  },
  "devDependencies": {
    "@opentelemetry/otlp-exporter-base": "catalog:",
    "@opentelemetry/otlp-grpc-exporter-base": "catalog:",
    "@storm-stack/nx": "workspace:*",
    "@types/node": "catalog:",
    "tsup": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
