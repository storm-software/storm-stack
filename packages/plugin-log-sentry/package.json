{
  "name": "@storm-stack/plugin-log-sentry",
  "version": "0.25.0",
  "type": "module",
  "description": "A package containing a Storm Stack log adapter to write log messages to Sentry",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-log-sentry"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      },
      "default": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "storm-stack-plugin",
    "storm-stack-log-plugin"
  ],
  "peerDependencies": {
    "@sentry/core": "catalog:",
    "@sentry/node": "catalog:",
    "@storm-stack/plugin-config": "*"
  },
  "peerDependenciesMeta": {
    "@sentry/core": {
      "optional": true
    },
    "@sentry/node": {
      "optional": true
    },
    "@storm-stack/plugin-config": {
      "optional": false
    }
  },
  "dependencies": {
    "@deepkit/type": "catalog:",
    "@storm-stack/core": "0.35.1",
    "@storm-stack/devkit": "0.11.1",
    "@stryke/json": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/nx": "0.16.1",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "6.0.0-dev.20250810"
  },
  "publishConfig": {
    "access": "public"
  }
}
