{
  "name": "utils-path",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/utils/path/src",
  "targets": {
    "build": {
      "executor": "@storm-software/workspace-tools:unbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "entry": "packages/utils/path/src/index.ts",
        "outputPath": "dist/packages/utils/path",
        "tsConfig": "packages/utils/path/tsconfig.json",
        "project": "packages/utils/path/package.json",
        "defaultConfiguration": "production",
        "platform": "node",
        "assets": [
          {
            "input": "packages/utils/path",
            "glob": "*.md",
            "output": "/"
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "/"
          }
        ]
      },
      "configurations": {
        "production": {
          "debug": false,
          "verbose": false
        },
        "development": {
          "debug": true,
          "verbose": true
        }
      }
    }
  },
  "tags": ["platform:node"]
}
