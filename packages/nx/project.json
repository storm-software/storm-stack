{
  "name": "nx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "packages/nx",
  "sourceRoot": "packages/nx/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx",
        "tsConfig": "packages/nx/tsconfig.lib.json",
        "project": "packages/nx/package.json",
        "main": "packages/nx/src/index.ts",
        "additionalEntryPoints": [
          "packages/nx/src/executors/ts-build/executor.ts",
          "packages/nx/src/executors/ts-server-build/executor.ts",
          "packages/nx/src/generators/config/generator.ts"
        ],
        "deleteOutputPath": true,
        "platform": "node",
        "bundle": true,
        "sourcemap": true,
        "minify": false,
        "skipTypeCheck": true,
        "format": ["cjs"],
        "esbuildOptions": {
          "outExtension": {
            ".js": ".js"
          }
        },
        "external": [
          "@storm-software/config",
          "@swc/wasm",
          "esbuild",
          "prettier",
          "nx"
        ],
        "assets": [
          {
            "input": "packages/nx",
            "glob": "*.md",
            "output": "/"
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "/"
          },
          {
            "input": "packages/nx/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "packages/nx/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "packages/nx",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "packages/nx",
            "glob": "executors.json",
            "output": "."
          },
          {
            "input": "packages/nx",
            "glob": "**/schema.json",
            "output": "."
          },
          {
            "input": "packages/nx",
            "glob": "**/files/**/{*.*,*}",
            "output": "."
          },
          {
            "input": "packages/nx/assets",
            "glob": "**/*.*",
            "output": "./assets"
          }
        ]
      }
    },
    "lint": {},
    "test": {}
  },
  "tags": []
}
