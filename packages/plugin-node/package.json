{
  "name": "@storm-stack/plugin-node",
  "version": "0.0.1",
  "type": "module",
  "description": "A package containing a Storm Stack NodeJs plugin for generating core NodeJs runtime functionality.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-node"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./renderers": {
      "import": {
        "types": "./dist/renderers/index.d.ts",
        "default": "./dist/renderers/index.js"
      },
      "require": {
        "types": "./dist/renderers/index.d.cts",
        "default": "./dist/renderers/index.cjs"
      },
      "default": {
        "types": "./dist/renderers/index.d.ts",
        "default": "./dist/renderers/index.js"
      }
    },
    "./renderers/*": {
      "import": {
        "types": "./dist/renderers/*.d.ts",
        "default": "./dist/renderers/*.js"
      },
      "require": {
        "types": "./dist/renderers/*.d.cts",
        "default": "./dist/renderers/*.cjs"
      },
      "default": {
        "types": "./dist/renderers/*.d.ts",
        "default": "./dist/renderers/*.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "storm-stack-plugin",
    "storm-stack-log-plugin"
  ],
  "peerDependencies": {
    "@storm-stack/plugin-dotenv": "*",
    "@storm-stack/plugin-error": "*",
    "@storm-stack/plugin-log-console": "*"
  },
  "peerDependenciesMeta": {
    "@storm-stack/plugin-dotenv": {
      "optional": false
    },
    "@storm-stack/plugin-error": {
      "optional": false
    },
    "@storm-stack/plugin-log-console": {
      "optional": false
    }
  },
  "dependencies": {
    "@babel/core": "catalog:",
    "@babel/helper-module-imports": "catalog:",
    "@babel/template": "catalog:",
    "@babel/types": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-stack/core": "workspace:*",
    "@storm-stack/devkit": "workspace:*",
    "@stryke/convert": "catalog:",
    "@stryke/json": "catalog:",
    "@stryke/path": "catalog:",
    "@stryke/string-format": "catalog:",
    "@stryke/type-checks": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/nx": "workspace:*",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "5.9.0-beta"
  },
  "publishConfig": {
    "access": "public"
  }
}
