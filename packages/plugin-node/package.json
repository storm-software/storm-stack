{
  "name": "@storm-stack/plugin-node",
  "version": "0.9.2",
  "type": "module",
  "description": "A Storm Stack plugin for creating applications using the NodeJs runtime.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-node"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      }
    },
    "./types/*": {
      "import": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.js"
      },
      "require": {
        "types": "./dist/types/*.d.cts",
        "default": "./dist/types/*.cjs"
      },
      "default": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.js"
      }
    }
  },
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "dependencies": {
    "@storm-software/build-tools": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-software/esbuild": "catalog:",
    "@stryke/env": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/hash": "catalog:",
    "@stryke/json": "catalog:",
    "@stryke/path": "catalog:",
    "bundle-require": "catalog:",
    "defu": "catalog:",
    "esbuild": "catalog:"
  },
  "devDependencies": {
    "@stryke/types": "catalog:",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "unimport": "catalog:",
    "unstorage": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
