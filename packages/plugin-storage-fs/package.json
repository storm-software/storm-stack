{
  "name": "@storm-stack/plugin-storage-fs",
  "version": "0.15.4",
  "type": "module",
  "description": "A package containing a Storm Stack log adapter to write logs to storage (for example: a file system)",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-storage-fs"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "require": {
        "types": "./dist/plugin.d.cts",
        "default": "./dist/plugin.cjs"
      },
      "default": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "storm-stack-plugin",
    "storm-stack-storage-plugin"
  ],
  "dependencies": {
    "@storm-stack/core": "workspace:*",
    "@storm-stack/devkit": "workspace:*",
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsup": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
