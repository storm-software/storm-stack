{
  "name": "@storm-stack/plugin-typedoc",
  "version": "0.1.0",
  "type": "module",
  "description": "A Typedoc plugin for generating documentation in Storm Stack packages.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "packages/plugin-typedoc"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "storm-stack",
    "storm-software",
    "typedoc",
    "typedoc-plugin",
    "typedocplugin"
  ],
  "peerDependencies": {
    "typedoc-plugin-frontmatter": "catalog:"
  },
  "peerDependenciesMeta": {
    "typedoc-plugin-frontmatter": {
      "optional": false
    }
  },
  "dependencies": {
    "handlebars": "catalog:",
    "typedoc": "catalog:",
    "typedoc-plugin-markdown": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
