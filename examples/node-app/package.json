{
  "name": "@storm-stack/examples-node-app",
  "version": "0.0.1",
  "type": "module",
  "description": "An example Storm Stack NodeJs application",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "examples/node-app"
  },
  "private": true,
  "dependencies": {
    "@storm-stack/log-console": "workspace:*",
    "@stryke/json": "catalog:",
    "@stryke/type-checks": "catalog:",
    "@stryke/url": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/core": "workspace:*",
    "@storm-stack/nx": "workspace:*",
    "@storm-stack/plugin-node": "workspace:*",
    "@stryke/types": "catalog:"
  },
  "storm": {
    "plugins": [
      [
        "@storm-stack/plugin-node",
        {
          "features": [
            "sentry",
            "env-paths"
          ]
        }
      ]
    ],
    "skipCache": true,
    "skipInstalls": true,
    "dotenv": {
      "types": {
        "variables": "./src/types.ts#StormNodeAppEnv"
      }
    }
  }
}
