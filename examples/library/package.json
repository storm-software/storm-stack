{
  "name": "@storm-stack/examples-library",
  "version": "0.0.1",
  "type": "module",
  "description": "An example Storm Stack library",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "examples/library"
  },
  "private": true,
  "dependencies": {
    "@storm-stack/core": "workspace:*",
    "@stryke/json": "catalog:",
    "@stryke/type-checks": "catalog:",
    "@stryke/url": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/devkit": "0.17.2",
    "@storm-stack/nx": "0.20.3",
    "@stryke/types": "catalog:"
  },
  "storm": {
    "plugins": [
      "@storm-stack/devkit/plugins/library"
    ],
    "skipCache": true,
    "skipInstalls": true,
    "platform": "node"
  }
}
