{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": [
      "@storm-stack/types",
      "node",
      "@cloudflare/workers-types/experimental"
    ],
    "outDir": "../../dist/out-tsc",
    "verbatimModuleSyntax": false
  },
  "include": ["src/**/*.ts", "src/**/*.js", ".storm/**/*.ts"],
  "exclude": ["jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"],
  "reflection": true
}
