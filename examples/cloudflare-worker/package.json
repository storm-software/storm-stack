{
  "name": "@storm-stack/examples-cloudflare-worker",
  "version": "0.0.1",
  "type": "module",
  "description": "An example Storm Stack Cloudflare Worker application",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "examples/cloudflare-worker"
  },
  "private": true,
  "dependencies": {
    "@cloudflare/unenv-preset": "catalog:",
    "@storm-stack/types": "workspace:*",
    "@stryke/json": "catalog:",
    "@stryke/type-checks": "catalog:",
    "@stryke/url": "catalog:",
    "unenv": "catalog:",
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/core": "0.41.1",
    "@storm-stack/nx": "0.19.5",
    "@storm-stack/plugin-cloudflare": "0.27.1",
    "@storm-stack/plugin-log-console": "0.23.1",
    "@stryke/types": "catalog:",
    "@types/node": "catalog:",
    "wrangler": "catalog:"
  },
  "storm": {
    "plugins": [
      "@storm-stack/plugin-cloudflare",
      [
        "@storm-stack/plugin-storage-cloudflare-kv",
        {
          "binding": "STORAGE",
          "namespace": "storage"
        }
      ]
    ],
    "output": {
      "outputMode": "fs"
    }
  }
}
