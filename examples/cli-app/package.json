{
  "name": "@storm-stack/examples-cli-app",
  "version": "0.0.1",
  "type": "module",
  "description": "An example Storm Stack commandline application",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "examples/cli-app"
  },
  "private": true,
  "bin": {
    "examples-cli": "dist/examples-cli.mjs"
  },
  "dependencies": {
    "@storm-stack/types": "workspace:*",
    "@stryke/cli": "catalog:",
    "@stryke/url": "catalog:",
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/core": "0.29.1",
    "@storm-stack/nx": "0.13.3",
    "@storm-stack/plugin-log-console": "0.14.3",
    "@storm-stack/plugin-log-sentry": "0.19.1",
    "@storm-stack/preset-cli": "0.20.1"
  },
  "storm": {
    "plugins": [
      "@storm-stack/plugin-log-sentry"
    ],
    "presets": [
      [
        "@storm-stack/preset-cli",
        {
          "bin": "examples-cli"
        }
      ]
    ],
    "skipCache": true,
    "skipInstalls": true,
    "dotenv": {
      "types": {
        "config": "./src/types.ts#StormCLIAppVariables"
      }
    }
  }
}
