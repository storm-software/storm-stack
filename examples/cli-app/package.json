{
  "name": "@storm-stack/examples-cli-app",
  "version": "0.0.1",
  "type": "module",
  "description": "An example Storm Stack commandline application",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-stack.git",
    "directory": "examples/cli-app"
  },
  "private": true,
  "bin": {
    "examples-cli-app": "dist/examples-cli-app.js"
  },
  "dependencies": {
    "@storm-stack/log-storage": "workspace:*",
    "@stryke/cli": "catalog:",
    "@stryke/url": "catalog:",
    "unstorage": "catalog:"
  },
  "devDependencies": {
    "@storm-stack/core": "workspace:*",
    "@storm-stack/nx": "workspace:*",
    "@storm-stack/preset-cli": "catalog:",
    "@storm-stack/types": "workspace:*"
  },
  "storm": {
    "plugins": [
      [
        "@storm-stack/preset-cli",
        {
          "features": [
            "sentry"
          ]
        }
      ]
    ],
    "skipCache": true,
    "skipInstalls": true,
    "skipLint": true,
    "mode": "development"
  }
}
